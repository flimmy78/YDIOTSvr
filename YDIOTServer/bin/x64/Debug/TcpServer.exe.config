<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <connectionStrings >
    <add name="connString" connectionString="server='(local)';database='Ryan';uid='sa';pwd='abc123==';Max Pool Size=100000;Min Pool Size=0;Connection Lifetime=0;packet size=32767;Connection Reset=false; async=true;"/>
  </connectionStrings>
  <appSettings>
    <!-- 轮询容忍时间 ms -->
    <add key="polling_delay_time" value="4000"/>
    <!-- 轮询时间 ms-->
    <add key="polling_time" value="8000"/>
    <!-- 初始化DTU个数 个-->
    <add key="DtuDicNumber" value="10000"/>
    <!-- TimerMgr 的个数 个-->
    <add key="TimerMgrNumber" value="30"/>
    <!-- TimerMgr 的时间间隔 ms-->
    <add key="TimerMgrInterval" value="2000"/>
    <!-- 任务数目超过ExceedNewThread 就开启子线程处理 单位个 -->
    <add key="ExceedNewThread" value="500000"/>
    <!-- 当前时间秒为ShowTaskNumber的整数倍就打印剩余任务数量 值为1-59 -->
    <add key="ShowTaskNumber" value="10"/>
    <!-- 一次开启SubThreadNunber个子线程  单位个-->
    <add key="SubThreadNunber" value="5"/>
    <!-- IfShowLog 为1开启log 否则不开  -->
    <add key="IfShowLog" value="1"/>
    <!-- 筛选connetId为 ShowFilterNumber 整数倍的打印  -->
    <add key="ShowFilterNumber" value="500"/>
    

  </appSettings>
</configuration>
